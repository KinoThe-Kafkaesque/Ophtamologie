<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Patient.
    -->
    <changeSet id="20220414224956-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="compte_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient__compte_id"
                                 referencedColumnNames="id"
                                 referencedTableName="compte"/>

        <addForeignKeyConstraint baseColumnNames="secretaire_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient__secretaire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="secretaire"/>

        <addForeignKeyConstraint baseColumnNames="maladie_id"
                                 baseTableName="patient"
                                 constraintName="fk_patient__maladie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="maladie"/>
    </changeSet>
</databaseChangeLog>
